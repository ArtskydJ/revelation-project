{
  "name": "revelation-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tape client/**/*-test.js",
    "build": "npm run create-symlinks && npm run glob && npm run build-css && npm run build-js",
    "build-js": "rollup -c && rollup -c -i client/lib/structure/data/structure.js -o public/static/structure.js && cp client/lib/sermons/sermons.json public/static/ && cp client/lib/timeline/data/timeline-data.json public/static/",
    "build-podcast": "node ./compile-podcast.js",
    "watch": "rollup -c -w --dev",
    "start": "serve public",
    "watch-glob": "watch \"npm run glob\" client/service client/view",
    "glob": "npm run glob-services && npm run glob-views",
    "glob-services": "glob-module-file --pattern=\"client/service/**/*.js\" --format=es --pathPrefix=../ --outputPath=client/globbed-services.js",
    "glob-views": "glob-module-file --pattern=\"client/view/**/*.js\" --format=es --pathPrefix=../ --outputPath=client/globbed-views.js",
    "create-symlinks": "mkdir -p client/node_modules/ && rm -r client/node_modules/ && mkdir client/node_modules/ && cd client/node_modules && ../../symlink.js ../component/ component && ../../symlink.js ../lib/ lib && cd ../../",
    "build-css": "node compile-css.js",
    "watch-css": "node compile-css.js watch",
    "deploy": "scp -r ./public/. revelation_bb@revelation.biblicalblueprints.org:/home/revelation_bb/revelation.biblicalblueprints.org",
    "update-sermons": "cd sermon-text-and-audio-correlater && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/KayserCommentaryOrg/revelation-project.git"
  },
  "author": "TehShrike",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/KayserCommentaryOrg/revelation-project/issues"
  },
  "homepage": "https://github.com/KayserCommentaryOrg/revelation-project#readme",
  "devDependencies": {
    "gaze": "1.1.2",
    "serve": "6.0.6",
    "watch": "1.0.2"
  },
  "dependencies": {
    "abstract-state-router": "~5.16.2",
    "asr-active-state-watcher": "~1.0.2",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "better-emitter": "1.0.2",
