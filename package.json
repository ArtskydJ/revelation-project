    "key-master": "~3.1.0",
    "majority-text-family-35-revelation": "~1.0.1",
    "make-dir": "1.0.0",
    "mannish": "~3.0.0",
    "multi-part-range-compare": "~1.0.3",
    "one-to-many-array-zip": "~1.0.2",
    "pify": "3.0.0",
